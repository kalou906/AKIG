{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend React pour AKIG",
  "private": true,
  "dependencies": {
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "akig-suite": "file:..",
    "axios": "^1.6.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "core-js": "^3.38.1",
    "dayjs": "^1.11.18",
    "framer-motion": "^10.16.16",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jotai": "^2.8.0",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.344.0",
    "normalize.css": "^8.0.1",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.0",
    "react-i18next": "^16.2.0",
    "react-lazyload": "^3.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.0",
    "react-scripts": "^5.0.1",
    "react-window": "^2.2.1",
    "recharts": "^2.12.0",
    "regenerator-runtime": "^0.14.1",
    "swr": "^2.2.5",
    "web-vitals": "^5.1.0",
    "whatwg-fetch": "^3.6.20",
    "zustand": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ui": "jest --testPathPattern=ui",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:fast": "playwright test --project=Chromium",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "db:reset": "ts-node ../backend/src/scripts/reset.ts",
    "db:migrate": "npm run --prefix ../backend db:migrate",
    "db:seed": "npm run --prefix ../backend db:seed"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "18.20.3",
    "npm": ">=10.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.9.1",
    "@types/prop-types": "^15.7.15",
    "@types/react-dom": "^19.2.2",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  }
}
