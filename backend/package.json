{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "start:guarded": "node src/scripts/start.js",
    "start:dev": "node src/index-dev.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev-simple": "node src/index-dev.js",
    "build": "tsc && cp -r src/migrations dist/src/",
    "migrate": "ts-node scripts/migrate.ts up",
    "migrate:down": "ts-node scripts/migrate.ts down",
    "migrate:test": "NODE_ENV=test ts-node scripts/migrate.ts up",
    "migrate:fix": "psql $DATABASE_URL -f src/migrations/01_fix_relations.sql",
    "rollback": "node src/scripts/rollback.js",
    "smoke": "node scripts/smoke-test.js",
    "verify": "node verify-environment.js",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "test:premium": "jest tests/premiumDashboard.e2e.test.js --forceExit",
    "test:integration": "jest --runInBand tests/integration",
    "test:ts": "jest -c jest.ts.config.js --runInBand",
    "lint": "eslint src --ext .js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
